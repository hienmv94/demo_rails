<% provide(:title, "Home") %>
<div class=".container-fluid">
<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="stats">
        <%= render 'shared/stats' %>
      </section>
      <section class="entry_form">
        <%= render 'shared/entry_form' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>New Feed</h3>
        <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <div class="row">
    <%= render 'shared/entries' %>
  </div>
  <%= link_to image_tag("rails.png", alt: "Rails logo"),'http://rubyonrails.org/' %>
<% end %>
</div>